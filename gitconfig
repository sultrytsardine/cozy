[alias]
  a = add
  aa = add --all
  alias = !git config -l | grep ^alias | cut -c 7- | sort
  amend = commit --amend --reuse-message=HEAD
  b = branch
  c = commit
  ci = commit -m
  co = checkout
  f = fetch
  fps = push --force
  l = log --graph --pretty=format:'%Cred%h%Creset -%C(yellow)%d%Creset %s %Cgreen(%cr) %C(bold blue)<%an>%Creset' --abbrev-commit
  m = merge
  pl = pull
  ps = push
  rb = rebase -i master
  s = status -s
  st = status

[core]
  editor = vim

[push]
  default = simple
